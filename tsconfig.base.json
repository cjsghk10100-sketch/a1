{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@agentapp/shared": ["packages/shared/src/index.ts"],
      "@agentapp/shared/*": ["packages/shared/src/*"]
    },
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node"]
  }
}
