{
  "name": "agent-app",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "desktop:dev": "pnpm -C apps/desktop dev",
    "desktop:dev:embedded": "DESKTOP_RUNNER_MODE=embedded pnpm -C apps/desktop dev",
    "desktop:dev:external": "DESKTOP_RUNNER_MODE=external pnpm -C apps/desktop dev",
    "desktop:dev:env": "set -a; [ -f .env.desktop ] && . ./.env.desktop; set +a; pnpm -C apps/desktop dev",
    "typecheck": "pnpm -r typecheck",
    "lint": "biome lint .",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "test": "echo \"(no tests yet)\""
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.11.0",
    "typescript": "^5.4.5"
  }
}
